<script> 
    $(document).ready(function () {

        createTable();

        function createTable() {

            $.ajax({
                url: "/home/getPracticeHistory", 
                method: "GET",
                data: {},
                success: (data) => {
                    console.log(data);
                    if(data.success === true){
                        $('#adminTable').html(data.data);
                    }
                    else{
                        $('#adminTable').html(data);
                    }
                
                }
            });

        }
});
</script>

    

  